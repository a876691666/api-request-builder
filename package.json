  "dependencies": {
    "css": "^2.2.3",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.1.0",
    "babel-preset-env": "^1.7.0",
    "gh-pages": "^1.2.0",
    "parcel-bundler": "^1.9.6",
    "postcss-modules": "^1.1.0",
    "postcss-preset-env": "^5.2.1",
